{
  "env": {
    "name": "#{env_name}"
  },
  "api": {
    "baseUrl": "#{api_baseUrl}"
  },
  "uiBackendApi": {
    "baseUrl": "#{uiBackendApi_baseUrl}"
  },
  "auth": {
    "baseUrl": "#{auth_baseUrl}",
    "realm": "#{auth_realm}",
    "clientId": "#{auth_clientId}"
  },
  "legal": {
    "version": "#{legal_version}",
    "copyrightOwner": "#{legal_copyrightOwner}"
  },
  "features": {
    "v2": {
      "multiplevaluedefinitions": "#{features_v2_multiplevaluedefinitions}",
      "multiplegroups": "#{features_v2_multiplegroups}",
      "dependentgroups": "#{features_v2_dependentgroups}",
      "timerestriction": "#{features_v2_timerestriction}"
    },
    "extra": {
      "displayvaluefiltericon": false,
      "showoptionspage": true
    }
  },
  "options": {
    "pollingtimeinseconds": 10,
    "pollingintervallinseconds": 1
  },
  "mock": {
    "terminology": false,
    "query": false,
    "result": false
  },
  "stylesheet": "primaryTheme"
}
