{
  "env": {
    "name": "dev"
  },
  "api": {
    "baseUrl": "/api"
  },
  "uiBackendApi": {
    "baseUrl": "http://localhost:8090/api/v3"
  },
  "auth": {
    "baseUrl": "http://localhost:8080",
    "realm": "feasibility",
    "clientId": "feasibility-webapp",
    "roles": ["FeasibilityUser"]
  },
  "legal": {
    "version": "1.0.0",
    "copyrightYear": "2023",
    "copyrightOwner": "MII Team"
  },
  "features": {
    "v2": {
      "multiplevaluedefinitions": true,
      "multiplegroups": false,
      "dependentgroups": true,
      "timerestriction": true
    },
    "extra": {
      "displayvaluefiltericon": false,
      "showoptionspage": true,
      "optionpageroles": ["FeasibilityUser"],
      "displayInfoMessage": true
    }
  },
  "options": {
    "sendsqcontexttobackend": true,
    "pollingtimeinseconds": 10,
    "pollingintervallinseconds": 10,
    "lowerboundarypatientresult": 10,
    "lowerboundarylocationresult": 1
  },
  "mock": {
    "terminology": false,
    "query": false,
    "result": false,
    "loadnsave": false
  },
  "stylesheet": "FDPGTheme",
  "fhirport": "8082",
  "dataset": "codex",
  "queryVersion": "v2"
}
