{
  "name": "num-portal-webapp",
  "version": "0.0.13",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "node --max_old_space_size=6144 ./node_modules/.bin/ng build",
    "test": "jest",
    "test-ci": "JEST_JUNIT_OUTPUT_DIR=./reports/junit/ node --expose-gc ./node_modules/.bin/jest --ci -w=1 --reporters=default --reporters=jest-junit",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "pretty-quick": "pretty-quick --pattern \"src/**/*.{scss,html}\"",
    "e2e": "ng e2e",
    "playground": "angular-playground"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "^15.2.0",
    "@angular-devkit/core": "^15.2.0",
    "@angular-devkit/schematics": "^15.2.0",
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.0",
    "@angular/material-moment-adapter": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@fortawesome/angular-fontawesome": "^0.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-regular-svg-icons": "^5.14.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@schematics/angular": "^15.2.0",
    "@types/file-saver": "^2.0.5",
    "@types/jasmine": "^4.3.0",
    "@types/jest": "^27.0.3",
    "angular-moment": "^1.3.0",
    "angular-oauth2-oidc": "^13.0.1",
    "angular-playground": "^10.1.0",
    "file-saver": "^2.0.5",
    "file-saver-es": "^2.0.5",
    "flag-icons": "^7.2.3",
    "jest": "^27.4.4",
    "jest-junit": "^13.0.0",
    "jest-preset-angular": "^11.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "monaco-editor": "0.29.1",
    "ng-lint-staged": "^12.0.3",
    "ngx-filesaver": "^15.0.0",
    "ngx-webstorage-service": "^4.1.0",
    "rxjs": "~7.8.0",
    "ts-jest": "^27.1.1",
    "tslib": "^2.3.1",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^15.2.1",
    "@angular-eslint/eslint-plugin": "^15.2.1",
    "@angular-eslint/eslint-plugin-template": "^15.2.1",
    "@angular-eslint/schematics": "^15.2.1",
    "@angular-eslint/template-parser": "^15.2.1",
    "@angular/cli": "^15.2.0",
    "@angular/compiler-cli": "^15.2.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.5",
    "@cucumber/cucumber": "^11.3.0",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "^16.11.12",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "cypress": "^14.4.1",
    "esbuild": "^0.25.5",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "eslint-plugin-prefer-arrow-functions": "^3.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "jasmine-core": "~3.10.1",
    "jasmine-marbles": "^0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "lint-staged": "^12.1.2",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "protractor": "~7.0.0",
    "ts-node": "~10.4.0",
    "typescript": "~4.8.4",
    "uuid": "~9.0.0",
    "webpack-dev-server": "^4.15.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern 'src/**/*.{scss,html,ts}' && lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts}": [
      "ng-lint-staged lint --fix --"
    ]
  },
  "jestSonar": {
    "reportPath": "reports/jest-sonar-reporter",
    "reportFile": "test-report.xml",
    "indent": 4
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/[filepath]/**/*.{js,ts}",
      "cypress/e2e/[filepath].{js,ts}",
      "cypress/support/step_definitions/**/*.{js,ts}"
    ]
  }
}
